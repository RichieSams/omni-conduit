{
  "schemaVersion": 1,
  "id": "omni-conduit",
  "version": "${version}",
  "name": "omni-conduit",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/omni-conduit/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "richiesams.omniconduit.OmniConduitModBase",
        "adapter": "kotlin"
      }
    ],
    "client": [
      {
        "value": "richiesams.omniconduit.OmniConduitClientBase",
        "adapter": "kotlin"
      }
    ],
    "fabric-datagen": [
      {
        "value": "richiesams.omniconduit.OmniConduitDataGenerator",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "omni-conduit.mixins.json",
    {
      "config": "omni-conduit.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.20.6",
    "java": ">=21",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.11.0+kotlin.2.0.0"
  },
  "suggests": {
    "another-mod": "*"
  }
}